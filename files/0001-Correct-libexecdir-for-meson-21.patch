From: Peter O'Connor <sunnyflunk@gmail.com>
Date: Mon, 21 Nov 2016 10:36:38 +1100
Subject: [PATCH 1/2] Correct libexecdir for meson (#21)

---
 ypkg2/rc.yml | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/ypkg2/rc.yml b/ypkg2/rc.yml
index b966493..1358055 100644
--- a/ypkg2/rc.yml
+++ b/ypkg2/rc.yml
@@ -17,7 +17,7 @@ actions:
         -DCMAKE_BUILD_TYPE=RelWithDebInfo -DCMAKE_INSTALL_PREFIX=%PREFIX%
     # Example: %meson_configure ..
     - meson_configure: |
-        CFLAGS="%CFLAGS%" CXXFLAGS="%CXXFLAGS%" LDFLAGS="%LDFLAGS%" meson --prefix %PREFIX% --buildtype=plain --libdir="lib%LIBSUFFIX%" --libexecdir="lib%LIBSUFFIX%%PKGNAME%"
+        CFLAGS="%CFLAGS%" CXXFLAGS="%CXXFLAGS%" LDFLAGS="%LDFLAGS%" meson --prefix %PREFIX% --buildtype=plain --libdir="lib%LIBSUFFIX%" --libexecdir="lib%LIBSUFFIX%/%PKGNAME%"
     - meson_build: |
         ninja %JOBS%
     - meson_install: |
